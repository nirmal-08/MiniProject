{
  "version": 2,
  "builds": [
    {
      "src": "MiniProject/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "MiniProject/wsgi.py"
    }
  ],
  "installCommand": "pip install -r requirements.txt"
}
